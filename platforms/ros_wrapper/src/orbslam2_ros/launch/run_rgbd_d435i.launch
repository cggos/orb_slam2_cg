<?xml version="1.0"?>

<launch>

  <arg name="orbvoc" default="$(find orbslam2_ros)/../../../../Vocabulary/ORBvoc.txt" />
  <arg name="config" default="$(find orbslam2_ros)/../../../app/RGB-D/RS_D435i_T265.yaml" />

  <node name="orbslam2_rgbd" pkg="orbslam2_ros" type="RGBD" args="$(arg orbvoc) $(arg config)" output="screen">
    <remap from="/camera/rgb/image_raw"              to="/D435I/color/image_raw" />
    <remap from="/camera/depth_registered/image_raw" to="/D435I/depth/image_rect_raw" />
  </node>

</launch>
